<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.getfit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GETFIT">
        <activity android:name=".Supplement.SupplementDashboard"></activity>
        <activity android:name=".Nutrition.macroCal" />
        <activity android:name=".Nutrition.AddMacros" />
        <activity android:name=".Nutrition.NutritionNavigate" />
        <activity android:name=".Nutrition.AddMeal" />
        <activity android:name=".Nutrition.UpdateMeal" />
        <activity android:name=".Nutrition.macros" />
        <activity android:name=".Nutrition.MealList" />
        <activity android:name=".Nutrition.ViewMeals" />
        <activity android:name=".Nutrition.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>