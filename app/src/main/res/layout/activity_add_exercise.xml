<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Workout.AddExercise">

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="365dp"
        android:layout_height="500dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/addExe_tv2"
            android:layout_width="117dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="40dp"
            android:fontFamily="@font/amaranth"
            android:text="@string/WorkOutId"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/addExe_tv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/addExe_tv2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="23dp"
            android:layout_marginTop="102dp"
            android:fontFamily="@font/amaranth"
            android:text="@string/WorkOutName"
            android:textAlignment="viewStart"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/addExe_tv4"
            android:layout_width="117dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/addExe_tv3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="159dp"
            android:fontFamily="@font/amaranth"
            android:text="@string/WorkOutPackageType"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/addExe_tv5"
            android:layout_width="117dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/addExe_tv6"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="214dp"
            android:layout_marginBottom="265dp"
            android:fontFamily="@font/amaranth"
            android:text="@string/WorkOutCalorieBurnt"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/addExe_tv6"
            android:layout_width="117dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/addExe_tv4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="265dp"
            android:fontFamily="@font/amaranth"
            android:text="@string/WorkOutDuration"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/addExe_tv7"
            android:layout_width="117dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="24dp"
            android:layout_marginBottom="152dp"
            android:fontFamily="@font/amaranth"
            android:text="@string/WorkOutsteps"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/addExe_etv1"
            android:layout_width="196dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="85dp"
            android:layout_marginRight="5dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/addExe_etn1"
            android:layout_width="196dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="20dp"
            android:layout_marginRight="7dp"
            android:ems="10"
            android:inputType="number" />

        <Spinner
            android:id="@+id/addExe_spinner"
            android:layout_width="199dp"
            android:layout_height="42dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="148dp"
            android:layout_marginRight="4dp"
            android:entries="@array/Spinner_Item"
            android:spinnerMode="dropdown" />

        <EditText
            android:id="@+id/addExe_etn2"
            android:layout_width="144dp"
            android:layout_height="34dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="261dp"
            android:layout_marginRight="62dp"
            android:ems="10"
            android:inputType="number" />

        <EditText
            android:id="@+id/addExe_etn3"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="205dp"
            android:layout_marginRight="6dp"
            android:ems="10"
            android:inputType="number" />

        <EditText
            android:id="@+id/addExe_emt1"
            android:layout_width="198dp"
            android:layout_height="171dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="9dp"
            android:layout_marginBottom="13dp"
            android:ems="10"
            android:gravity="start|top"
            android:inputType="textMultiLine" />

        <TextView
            android:id="@+id/addExe_tv8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="29dp"
            android:layout_marginBottom="209dp"
            android:text="@string/WorkOutminutes" />

    </RelativeLayout>

    <TextView
        android:id="@+id/addExe_tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/black_ops_one"
        android:text="@string/WorkOutAddExercise"
        android:textColor="@color/black"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.74" />

    <Button
        android:id="@+id/addExe_btn"
        android:layout_width="227dp"
        android:layout_height="37dp"
        android:layout_marginStart="160dp"
        android:text="@string/WorkOutAddBtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout1"
        app:layout_constraintVertical_bias="0.205" />



</androidx.constraintlayout.widget.ConstraintLayout>